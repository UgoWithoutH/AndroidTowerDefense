<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidtowerdefense">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tower"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tower"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidTowerDefense">

        <activity android:name=".modelandroid.GameActivity"></activity>
        <activity android:name=".modelandroid.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".modelandroid.GameActivity"/>
    </application>

</manifest>